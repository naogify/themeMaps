    {
      "id": "ふじのくに/観光施設等一覧-Polygon",
      "type": "fill",
      "source": "menu-experiment",
      "source-layer": "SightseeingMapStore",
      "filter": [
        "==",
        [
          "geometry-type"
        ],
        "Polygon"
      ],
      "paint": {
        "fill-color": "#a6054d",
        "fill-opacity": 0.7
      },
